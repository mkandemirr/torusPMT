#stainlessSteel_os
/control/alias osName stainlessSteel
/control/alias dataDirectory torusPMT/data/surface_mpt/{osName}

/mpt/{osName}/addProperty REFLECTIVITY {dataDirectory}/reflectivity.txt nm unitless 

/os/{osName}/model glisur 
/os/{osName}/type dielectric_metal
/os/{osName}/finish ground
/os/{osName}/polish 0.1








